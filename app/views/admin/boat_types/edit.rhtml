<% content_for :title do %>Edit Boat Type<% end %>

<h1>Edit Type</h1>
<%= error_messages_for :boat_type %>

<% form_for(:boat_type, :url => admin_boat_type_path(@boat_type), :html=>{:method=>:put}) do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>
<span id="required">*</span>-Required <br/>
<%= f.submit "Update" %>
<% end %>
  
<%= link_to 'Back', admin_boat_types_path %>