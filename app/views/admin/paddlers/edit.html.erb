<% content_for :title do %>Edit Paddler<% end %>
<div>
  <%= render :partial => 'admin/teams/submenu/menu'%>
</div>

<h1>Edit Paddler</h1>
<%= error_messages_for :paddler %>

<% form_for(:paddler, :url => admin_paddler_path(@team,@paddler.id), :html=>{:method=>:put}) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <span id="required">*</span>-Required <br/>
  <%= f.submit "Update" %>
<% end %>

<%= link_to 'Show', admin_paddler_path(@team,@paddler.id) %> |
<%= link_to 'Back', admin_paddlers_path(@team)%>
