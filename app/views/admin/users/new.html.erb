  <h1>New user</h1>
  
  <%= error_messages_for :user %>
  
  
  <% form_for(:user, :url => admin_users_path) do |f| %>
  
  <table id="users" class="form">
  <%= render :partial => 'form', :locals => { :f => f } %>
  </table>

  <span id="required">*</span>-Required <br/>
  <%= f.submit "Create" %>
  <% end %>
  
<%= link_to 'Back', admin_users_path %>
