<% content_for :title do %>Boat <%=@team.name%> - Select Extras<% end %>
<div>
    <h2>Boat: <%=@team.name%></h2>  
     <fieldset>
      <legend>Step 3 from 5 - Add Extras</legend>
      <% form_for :boat, :url => { :action => 'add_extras', :id=>@team.id }, :html => { :multipart => true } do |f| %>
   
      <%= render :partial => 'extras_item', :locals=>{:mode=>:write}%>
     

    
        <div id="buttons">
            <%= submit_tag  "Checkout"%><br/>
            <%# link_to("Cancel", member_new_boat_path ) %> 
        </div>
    <%end%>
    </fieldset>
  </div>