<% content_for :title do %>Boats List<% end %>

<% content_for :breadcrumbing_area do %>
<h1>Select Boat</h1>
<% end %>

<div id="boat_list">
 <table class="box">
  <tr>
    <td><h2>Boats</h2></td>
    <td width="200"><%= link_to "Register Another Team", register_user_url, :class=>"menu"%></td>
  </tr>

  <% @teams.each do |team|%>
   <tr>
     <td colspan="2" class="specialDay-current_reserved"><%=link_to team.name,  member_boat_url(team)%></td>
   </tr>
   <%end%>
   </table>
</div>
  