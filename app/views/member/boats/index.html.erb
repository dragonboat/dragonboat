<% content_for :title do %>Manage: <%=CGI.unescapeHTML(@team.name)%><% end %>

<div>
  <%= render :partial => 'member/boats/submenu/menu'%>
</div>
<p>Team Name: <%=@team.name%></p>
<p>Team URL: <%=link_to @team.url, @team.url%></p>
<p>Team Biography: <%=@team.description%></p>
<p>Captain: <%=@team.captain.person.name%></p>
<p>Created at: <%=@team.created_at.strftime("%d/%m/%Y")%></p>
<p>Members count: 0</p>
<p>Logo: <%= image_tag( @team.image.public_filename(:thumb_big) ) if @team.image%> </p>
