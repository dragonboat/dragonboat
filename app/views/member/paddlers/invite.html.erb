<% content_for :title do %>Manage Orphaned Paddlers : Invite to join team<% end %>
<div>
  <%= render :partial => 'member/boats/submenu/menu'%>
</div>
<h1>Invite to join team</h1>
<div class="instructions">
  <%= render :comatose=>'invite-to-join-team-instructions', :silent=>true %>
</div>
<%= error_messages_for :member %>

<% form_for(:member, :url => member_create_member_path(@team,@paddler.id), :html=>{:method=>:put}) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <span id="required">*</span>-Required <br/>
  <%= f.submit "Confirm" %>
<% end %>

<%= link_to 'Details', member_paddler_path(@team,@paddler.id) %> |
<%= link_to 'Back', member_paddlers_path(@team) %>