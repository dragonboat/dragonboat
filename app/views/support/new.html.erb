<% content_for :title do %>Support : New Ticket<% end %>
  <div>
     <fieldset>
            <legend>Create Ticket</legend>
            
            <% form_for :ticket, :url => { :action => 'create' }, :html => { :multipart => true } do |f| %>
            <%= render :partial => 'form', :locals =>{:f=>f} %>
            <p><span id="required">*</span>-Required </p>
            <div id="buttons">
              <%= submit_tag  "Send"%>
          </div>
      <%end%>
    </fieldset>
   </div>